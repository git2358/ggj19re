pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- jacob cazabon 2019

local t,f=true,false
local gs,gt
local px,py,ps,pw,pl,pwt,pct
local cx,cy,ctx,cty
local fx,fy,fr1,fr2,ft,fl
local hud,pdw
local ir

function _init(_gs)

	if (_gs==nil) _gs=0
	
	reload()
	
	-- game
	gs,gt=_gs,0
	ir=-4
	
	-- player
	px,py=940,26
	ps=0b010
	pl,pw=100,0
	pwt,pct=0,0
	
	-- campfire
	fx,fy=940,28
	fl,ft=1680,0
	fr1,fr2=24,84
	
	-- camera
	ctx,cty=fx,fy
	cx,cy=ctx-64,cty-64
	
	-- hud
	hud,pdw=0,pw
	
end

function _update()

	-- player movement
	local px0,py0=px,py
	local wsh=btn(0)==btn(1)
	local wsv=btn(2)==btn(3)
	if band(ps,1)>0 then
		pwt+=1
		if not wsh then
			if btn(0) then
				px-=1
				ps=bor(ps,2)
			end
			if btn(1) then
			 px+=1
			 ps=band(ps,bnot(2))
			end
 	end
 	if not wsv then
 		if (btn(2)) py-=1
 		if (btn(3)) py+=1
 	end
	end
	if (wsh and wsv) pwt=0
	
	-- collision
	local ptx,pty
	local tax,tay,tad,tox,toy
	local f0,f1,f2
	ptx,pty=flr(px0/8),flr(py0/8)
	tox=px0%8<4 and -1 or 1
	toy=py0%8<4 and -1 or 1
	tax=mget(ptx+tox,pty)
	tay=mget(ptx,pty+toy)
	tad=mget(ptx+tox,pty+toy)
	f0=fget(tax,0)
	f1=fget(tax,1)
	f2=fget(tax,2)
	if (f1 and f2) or (f0 and
		(not f1) and (not f2))
	then
		if (tox<0) px=max(px,ptx*8+2)
		if (tox>0) px=min(px,ptx*8+5)
	end
	f0=fget(tay,0)
	f1=fget(tay,1)
	f2=fget(tay,2)
	if (f1 and f2) or (f0 and
		(not f1) and (not f2))
	then
		if (toy<0) py=max(py,pty*8+1)
		if (toy>0) py=min(py,pty*8+6)
	end
	f0=fget(tad,0)
	f1=fget(tad,1)
	f2=fget(tad,2)
	if (f1 and f2) or (f0 and
		(not f1) and (not f2)) and
		flr(px0/8)!=flr(px/8) and
		flr(py0/8)!=flr(py/8)
	then
		if (toy<0) py=max(py,pty*8+1)
 	if (toy>0) py=min(py,pty*8+6)
	end
	
	-- half collision
	local t=mget(ptx,pty)
	local fn=0
	if (fget(t,0)) fn+=4
	if (fget(t,1)) fn+=2
	if (fget(t,2)) fn+=1
	if (fn==1) px=min(px,ptx*8+3)
	if (fn==2) px=max(px,ptx*8+5)
	if (fn==5) py=min(py,pty*8+3)
	if (fn==6) py=max(py,pty*8+5)
	
	-- hud
	if band(ps,1)>0 then
		hud=min(hud+1,7)
 	if (pdw>pw) pdw-=1
 	if (pdw<pw) pdw+=1
	else
		hud=max(hud-1,0)
	end
	if (hud<=0) pdw=pw
	
	-- healing/damage tick
	local heal,dmg
	heal=fx!=nil and fy!=nil
	if heal then
		local fd
 	fd=sqrt((fx-px)^2+(fy-py)^2)
 	heal=0<=fd and fd<fr1
 end
 dmg=fx==nil or fy==nil
 if not dmg then
 	local fd
 	fd=sqrt((fx-px)^2+(fy-py)^2)
 	dmg=fd<0 or fr2<fd
 end
 if band(ps,1)>0 then
 	if heal then
  	pl=min(pl+0.34,100)
  elseif dmg then
  	pl=max(pl-0.5,0)
  else
  	pl=-flr(-pl)
  end
 end
 if pl<=0 and gs!=-1 then
 	gs,gt=-1,0
 end
	
	-- bridges
	f0=fget(t,0)
	f1=fget(t,1)
	f2=fget(t,2)
	if btn(4) and f1!=f2 and
		pw>=10 and band(ps,1)>0
	then
		if fn==1 then
			pw-=10
			mset(ptx,pty,t-16)
			if mget(ptx+1,pty)==t-32 then
				mset(ptx+1,pty,t-16)
			else
				mset(ptx+1,pty,t)
			end
		end
		if fn==2 then
			pw-=10
			mset(ptx,pty,t+16)
			if mget(ptx-1,pty)==t+32 then
				mset(ptx-1,pty,t+16)
			else
				mset(ptx-1,pty,t)
			end
		end
		if fn==5 then
			pw-=10
			mset(ptx,pty,t-16)
			if mget(ptx,pty+1)==t-32 then
				mset(ptx,pty+1,t-16)
			else
				mset(ptx,pty+1,t)
			end
		end
		if fn==6 then
			pw-=10
			mset(ptx,pty,t+16)
			if mget(ptx,pty-1)==t+32 then
				mset(ptx,pty-1,t+16)
			else
				mset(ptx,pty-1,t)
			end
		end
	end
	
	-- campfires
	if fx!=nil and fy!=nil then
		ft+=1
		if band(ps,1)>0 then
 		fl-=1
 		if (fl<0) fx,fy=nil,nil
 		fr1=-0.04*(1680-fl)+24
 		fr2=-0.05*(1680-fl)+84
 	end
	end
	if btn(4) and band(ps,1)>0 and
		not fget(mget(ptx,pty),4)
	then
		local ok=fx==nil or fy==nil
		if not ok then
  	local fd
  	fd=sqrt((fx-px)^2+(fy-py)^2)
 		ok=fd<0 or fr2<fd
 	end
 	if ok and pw>=3 then
  	fx,fy=px,py
  	fl,ft=1680,0
  	fr1,fr2=24,84
  	pw-=3
  end
	end
	
	-- chop trees
	local ptx2,pty2,t2
	ptx2,pty2=ptx,flr(py/8)
	t2=mget(ptx2,pty2)
	if btn(5) and
		band(ps,1)>0
	then
		if fget(t2,3) and pct<=0 then
			pct=8
			pw+=1
			mset(ptx2,pty2,t2+1)
		end
	else
		pct=0
	end
	pct=max(pct-1,0)
	
	-- camera
	if gs==0 or gs==1 then
		ctx,cty=fx,fy
		cx,cy=ctx-64,cty-64
	end
	if band(ps,4)>0 then
		ctx,cty=px,py
	end
	cx+=flr((ctx-cx-64)/2+0.5)
	cy+=flr((cty-cy-64)/2+0.5)
	if (ctx-cx==63) cx=ctx-64
	if (cty-cy==63) cy=cty-64
	
	-- game state
	if gs==0 then
		ir=min(ir+1,28)
		if (ir>=28) gs,gt=1,0
	end
	if gs==1 then
		if btn(4) then
			gs,gt=2,0
			ps=bor(ps,1)
		end
	end
	if gs==2 then
		ir=min(ir+2,92)
		if py>=88 then
			ps=bor(ps,4)
		end
	end
	if gs==-1 then
		ps=band(ps,bnot(1))
		if gt>=90 then
			ir=max(ir-4,-4)
			if ir<=-4 then
				_init(0)
			end
		end
	end
	gt+=1

end

function frdr()
	local fs=ft%20<10 and 12 or 11
	if fl<1080 then
		fs=ft%40<20 and 14 or 13
	end
	spr(fs,fx-cx-4,fy-cy-4)
end

function _draw()

	if ir!=nil and ir<91 then
		cls(0)
		clip(60-ir,60-ir,ir*2+8,
			ir*2+8)
		circfill(64,64,ir+8,1)
	else
		cls(1)
		clip()
	end
	
	-- map
	local mtx,mty,mx,my
	mtx=flr(cx/8)-1
	mty=flr(cy/8)-1
	mx,my=-cx+mtx*8,-cy+mty*8
	map(mtx,mty,mx,my,18,18)
	
	-- campfire behind player
	if fx!=nil and fy!=nil and
		fy<py
	then
		frdr()
	end
	
	-- player
	if gs!=-1 then
 	local pxo
 	pxo=band(ps,2)==0 and 4 or 3
 	spr((pwt+8)%18<9 and 1 or 2,
 		px-pxo-cx,py-cy-7,1,1,
 		band(ps,2)==0)
 else
 	if gt<30 then
  	local pxo
  	pxo=band(ps,2)==0 and 4 or 3
  	spr(1,px-pxo-cx,py-cy-7,1,1,
  		band(ps,2)==0)
 	elseif gt<60 then
  	local pxo
   pxo=band(ps,2)==0 and 4 or 3
   spr(3,px-pxo-cx,py-cy-7,1,1,
   	band(ps,2)==0)
 	else
 		local pxo
  	pxo=band(ps,2)==0 and 2 or 5
  	spr(4,px-pxo-cx,py-cy-7,1,1,
  		band(ps,2)==0)
 	end
 end
		
	-- campfire in front of player
	if fx!=nil and fy!=nil and
		fy>=py
	then
		frdr()
	end
	
	-- campfire circles
	if fx!=nil and fy!=nil then
 	if ft%20<10 then
 		fillp(0xa5a5+0b.1)
 	else
 		fillp(0x5a5a+0b.1)
 	end
 	if ir+4>=fr1 then
 		circ(fx-cx,fy-cy,fr1,9)
 	end
 	if ir+4>=fr2 then
  	circ(fx-cx,fy-cy,fr2-1,10)
  	circ(fx-cx,fy-cy,fr2,10)
  	circ(fx-cx,fy-cy,fr2+2,10)
  end
 	fillp()
 end
 
 -- hud
 local phc
 phc=14
	local heal,dmg
	heal=fx!=nil and fy!=nil
	if heal then
		local fd
 	fd=sqrt((fx-px)^2+(fy-py)^2)
 	heal=0<=fd and fd<fr1
 end
 dmg=fx==nil or fy==nil
 if not dmg then
 	local fd
 	fd=sqrt((fx-px)^2+(fy-py)^2)
 	dmg=fd<0 or fr2<fd
 end
 if band(ps,1)>0 then
 	if heal then
  	phc=9
  elseif dmg then
  	phc=gt%10<5 and 8 or 14
  end
 end
 local plm="â™¥"..-flr(-pl)
 rectfill(0,128-hud,127,128,0)
 print(plm,108,129-hud,phc)
 spr(15,-1,126-hud)
 print(pdw,8,129-hud,4)
 local d,m=pw-pdw
 local pwx=4*#(""..pdw)
 if d>0 then
 	m="+"..abs(d)
 	print(m,8+pwx,129-hud,11)
 elseif d<0 then
 	m="-"..abs(d)
 	print(m,8+pwx,129-hud,8)
 end
 
 -- black circle
 clip()
 if ir!=nil and ir<91 then
 	if ft%20<10 then
 		fillp(0xa5a5+0b.1)
 	else
 		fillp(0x5a5a+0b.1)
 	end
 	circ(64,64,ir,0)
 	circ(64,64,ir+1,0)
 	circ(64,64,ir+3,0)
 	circ(64,64,ir+4,0)
 	fillp()
 	for r=5,max(8,ir*1.41) do
 		circ(63,63,ir+r,0)
 		circ(64,65,ir+r,0)
 		circ(65,65,ir+r,0)
 	end
 end
 
 print(gs,1,1,14)
 print(gt,1,7,12)
	
end
